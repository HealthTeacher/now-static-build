{
  "builds": [
    { "src": "packages/site/package.json", "use": "@now/static-build" },
    {
      "src": "packages/site/index.js",
      "use": "@now/node",
      "config": {
        "includeFiles": ["packages/site/src/**"]
      }
    }
  ],
  "routes": [{ "src": "/.*", "dest": "packages/site/index.js" }]
}
